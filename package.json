{
  "name": "kasane-client",
  "version": "0.0.2",
  "description": "TypeScript wrapper for Kasane 4-dimensional space-time database engine",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "README_JA.md",
    "TUTORIAL_JA.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "npx tsx ./demo/sample.ts"
  },
  "keywords": [
    "spatial",
    "temporal",
    "database",
    "4d",
    "space-time",
    "wasm",
    "webassembly",
    "gis",
    "spatial-data",
    "time-series"
  ],
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.3.3"
  }
}